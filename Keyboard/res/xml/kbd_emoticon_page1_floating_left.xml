<?xml version="1.0" encoding="utf-8" ?>
<Keyboard xmlns:latin="http://schemas.android.com/apk/res/com.pantech.inputmethod.skyime" latin:keyboardTopPadding="@dimen/floating_qwerty_top_padding" latin:rowHeight="@dimen/floating_qwerty_keyheight" latin:keyWidth="25.0%p">
	<include latin:keyboardLayout="@xml/kbd_key_styles" />
	<switch>
		<case latin:hideEmojiMode="true">
			<Row latin:rowHeight="@dimen/top_num_keyheight" latin:keyWidth="20.000004%p">
				<Key latin:backgroundType="topnumber" latin:keyLabel="1" />
				<Key latin:backgroundType="topnumber" latin:keyLabel="2" />
				<Key latin:backgroundType="topnumber" latin:keyLabel="3" />
				<Key latin:backgroundType="topnumber" latin:keyLabel="4" />
				<Key latin:backgroundType="topnumber" latin:keyLabel="5" />
			</Row>
		</case>
		<default>
			<Row latin:rowHeight="@dimen/top_num_keyheight">
				<Key latin:backgroundType="emojiTab" latin:keyStyle="switchNumSymKeyStyle" />
				<Key latin:backgroundType="emojiTab" latin:keyStyle="switchEmojiRecentsKeyStyle" />
				<Key latin:backgroundType="emojiTab" latin:keyStyle="switchEmojiPeopleKeyStyle" />
				<Key latin:backgroundType="emojiTab" latin:keyStyle="switchEmojiObjectsKeyStyle" />
			</Row>
		</default>
	</switch>
	<Row>
		<Key latin:code="@integer/key_symbol_01" latin:keyStyle="emoticonKeyStyle" />
		<Key latin:code="@integer/key_symbol_02" latin:keyStyle="emoticonKeyStyle" />
		<Key latin:code="@integer/key_symbol_03" latin:keyStyle="emoticonKeyStyle" />
		<Key latin:code="@integer/key_symbol_04" latin:keyStyle="emoticonKeyStyle" />
	</Row>
	<Row>
		<Key latin:code="@integer/key_symbol_09" latin:keyStyle="emoticonKeyStyle" />
		<Key latin:code="@integer/key_symbol_10" latin:keyStyle="emoticonKeyStyle" />
		<Key latin:code="@integer/key_symbol_11" latin:keyStyle="emoticonKeyStyle" />
		<Key latin:code="@integer/key_symbol_12" latin:keyStyle="emoticonKeyStyle" />
	</Row>
	<Row>
		<Key latin:keyLabel="1/5" latin:keyStyle="symbolPageViewStyle" />
		<Key latin:code="@integer/key_symbol_17" latin:keyStyle="emoticonKeyStyle" />
		<Key latin:code="@integer/key_symbol_18" latin:keyStyle="emoticonKeyStyle" />
		<Key latin:code="@integer/key_symbol_19" latin:keyStyle="emoticonKeyStyle" />
	</Row>
	<Row latin:rowHeight="@dimen/floating_common_keyheight">
		<Key latin:keyStyle="switchKorEngKeyStyle" />
		<Key latin:keyStyle="symbolPrevKeyStyle" latin:keyWidth="50.0%p" />
		<Key latin:keyStyle="spaceKeyStyle" />
	</Row>
</Keyboard>
