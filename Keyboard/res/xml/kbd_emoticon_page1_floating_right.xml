<?xml version="1.0" encoding="utf-8" ?>
<Keyboard xmlns:latin="http://schemas.android.com/apk/res/com.pantech.inputmethod.skyime" latin:keyboardTopPadding="@dimen/floating_qwerty_top_padding" latin:rowHeight="@dimen/floating_qwerty_keyheight" latin:keyWidth="25.0%p">
	<include latin:keyboardLayout="@xml/kbd_key_styles" />
	<switch>
		<case latin:hideEmojiMode="true">
			<Row latin:rowHeight="@dimen/top_num_keyheight" latin:keyWidth="20.000004%p">
				<Key latin:backgroundType="topnumber" latin:keyLabel="6" />
				<Key latin:backgroundType="topnumber" latin:keyLabel="7" />
				<Key latin:backgroundType="topnumber" latin:keyLabel="8" />
				<Key latin:backgroundType="topnumber" latin:keyLabel="9" />
				<Key latin:backgroundType="topnumber" latin:keyLabel="0" />
			</Row>
		</case>
		<default>
			<Row latin:rowHeight="@dimen/top_num_keyheight" latin:keyWidth="25.0%p">
				<Key latin:backgroundType="emojiTab" latin:keyStyle="switchEmojiNatureKeyStyle" />
				<Key latin:backgroundType="emojiTab" latin:keyStyle="switchEmojiPlacesKeyStyle" />
				<Key latin:backgroundType="emojiTab" latin:keyStyle="switchEmojiSymbolsKeyStyle" />
				<Key latin:backgroundType="emojiTab" latin:keyStyle="switchEmojiEmoticonKeyStyle" />
			</Row>
		</default>
	</switch>
	<Row>
		<Key latin:code="@integer/key_symbol_05" latin:keyStyle="emoticonKeyStyle" />
		<Key latin:code="@integer/key_symbol_06" latin:keyStyle="emoticonKeyStyle" />
		<Key latin:code="@integer/key_symbol_07" latin:keyStyle="emoticonKeyStyle" />
		<Key latin:code="@integer/key_symbol_08" latin:keyStyle="emoticonKeyStyle" />
	</Row>
	<Row>
		<Key latin:code="@integer/key_symbol_13" latin:keyStyle="emoticonKeyStyle" />
		<Key latin:code="@integer/key_symbol_14" latin:keyStyle="emoticonKeyStyle" />
		<Key latin:code="@integer/key_symbol_15" latin:keyStyle="emoticonKeyStyle" />
		<Key latin:code="@integer/key_symbol_16" latin:keyStyle="emoticonKeyStyle" />
	</Row>
	<Row>
		<Key latin:code="@integer/key_symbol_20" latin:keyStyle="emoticonKeyStyle" />
		<Key latin:code="@integer/key_symbol_21" latin:keyStyle="emoticonKeyStyle" />
		<Key latin:code="@integer/key_symbol_22" latin:keyStyle="emoticonKeyStyle" />
		<Key latin:keyStyle="deleteKeyStyle" />
	</Row>
	<Row latin:rowHeight="@dimen/floating_common_keyheight">
		<Key latin:keyStyle="spaceKeyStyle" />
		<Key latin:keyStyle="symbolNextKeyStyle" latin:keyWidth="50.0%p" />
		<Key latin:keyStyle="returnKeyStyle" />
	</Row>
</Keyboard>
