<?xml version="1.0" encoding="utf-8" ?>
<merge xmlns:latin="http://schemas.android.com/apk/res/com.pantech.inputmethod.skyime">
	<key-style latin:backgroundType="normal" latin:keyLabelOption="followKeyLetterRatio|largeLetter" latin:styleName="numKeyStyle" />
	<key-style latin:keyLabelOption="followKeyLetterRatio|fontNormal" latin:styleName="numModeKeyStyle" />
	<key-style latin:backgroundType="functional" latin:keyLabelOption="followKeyLetterRatio|largeLetter" latin:styleName="numFunctionalKeyStyle" />
	<key-style latin:styleName="numberKeyStyle" latin:parentStyle="numKeyStyle" />
	<key-style latin:code="48" latin:keyLabel="0" latin:styleName="num0KeyStyle" latin:parentStyle="numberKeyStyle" />
	<key-style latin:keyLabel="1" latin:styleName="num1KeyStyle" latin:parentStyle="numberKeyStyle" />
	<key-style latin:keyLabel="2" latin:styleName="num2KeyStyle" latin:parentStyle="numberKeyStyle" />
	<key-style latin:keyLabel="3" latin:styleName="num3KeyStyle" latin:parentStyle="numberKeyStyle" />
	<key-style latin:keyLabel="4" latin:styleName="num4KeyStyle" latin:parentStyle="numberKeyStyle" />
	<key-style latin:keyLabel="5" latin:styleName="num5KeyStyle" latin:parentStyle="numberKeyStyle" />
	<key-style latin:keyLabel="6" latin:styleName="num6KeyStyle" latin:parentStyle="numberKeyStyle" />
	<key-style latin:keyLabel="7" latin:styleName="num7KeyStyle" latin:parentStyle="numberKeyStyle" />
	<key-style latin:keyLabel="8" latin:styleName="num8KeyStyle" latin:parentStyle="numberKeyStyle" />
	<key-style latin:keyLabel="9" latin:styleName="num9KeyStyle" latin:parentStyle="numberKeyStyle" />
	<key-style latin:code="42" latin:keyLabel="*" latin:styleName="numStarKeyStyle" latin:parentStyle="numKeyStyle" />
	<key-style latin:code="0x23" latin:keyLabel="\#" latin:styleName="numHashKeyStyle" latin:parentStyle="numKeyStyle" />
	<key-style latin:code="@integer/key_shift" latin:keyLabel="@string/label_to_phone_symbols_key" latin:styleName="numSwitchToAltKeyStyle" latin:parentStyle="numModeKeyStyle" />
	<key-style latin:code="@integer/key_shift" latin:keyLabel="@string/label_to_phone_numeric_key" latin:styleName="numSwitchToNumericKeyStyle" latin:parentStyle="numModeKeyStyle" />
	<key-style latin:code="@integer/key_space" latin:isRepeatable="true" latin:keyIcon="iconSpaceKey" latin:styleName="numSpaceKeyStyle" latin:parentStyle="numFunctionalKeyStyle" />
	<key-style latin:code="@integer/key_space" latin:isRepeatable="true" latin:keyIcon="iconSpaceFloatingKey" latin:styleName="numSpacePortraitFloatingKeyStyle" latin:parentStyle="numFunctionalKeyStyle" />
	<switch>
		<case latin:clobberSettingsKey="true">
			<key-style latin:styleName="numFunctionalMoreKeyStyle" latin:parentStyle="numFunctionalKeyStyle" />
		</case>
		<default>
			<key-style latin:moreKeys="settings" latin:keyIconHint="iconHintSettingKey" latin:styleName="numFunctionalMoreKeyStyle" latin:parentStyle="numFunctionalKeyStyle" />
		</default>
	</switch>
	<key-style latin:moreKeys="settings" latin:keyIconHint="iconHintFloatingSettingKey" latin:styleName="numFunctionalMorePortraitFloatingKeyStyle" latin:parentStyle="numFunctionalKeyStyle" />
</merge>
